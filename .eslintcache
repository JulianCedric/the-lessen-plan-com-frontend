urn (  \n            <div>\n                {this.props.lessenPlans.map(lessenPlan => <LessenPlan key={lessenPlan.id} lessenPlan={lessenPlan} ref={(el) => (this.componentRef = el)} delete={this.props.delete}/>)}\n            \n                {this.state.renderPrint ? <Print /> : null }\n\n            </div>\n        );\n    }\n}\n \nexport default LessenPlans; ","/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Create.js",["115"],"import React from 'react';\nimport { Button, Form, Grid, Icon, Segment } from 'semantic-ui-react';\n\nclass Create extends React.Component {\n    state = {  \n        id: 1,\n        user_id: 1,\n        type: \"\",\n        habit: \"\",\n        dueDate: \"\",\n        habitDesc: \"\",\n        habitBad: false,\n        habitLoopItemA: \"\",\n        habitLoopItemB: \"\",\n        habitLoopItemC: \"\",\n        habitLoopItemD: \"\",\n        habitTracker: \"\"\n    }\n\n    handleChangeHabit = e => {\n        console.log(e.target.name, e.target.value)\n        this.setState({ [e.target.name]: e.target.value })\n    }\n\n    handleSubmitHabit = e => {\n        e.preventDefault()\n        let obj = this.state\n        this.props.create(obj)\n        this.props.renderCreate()\n    }\n\n    render() { \n        return (  \n            <div className=\"create\">\n                <div className=\"twelve wide column\">\n                    <Grid columns={1} relaxed='very' stackable style={{ height: '0vh' }} verticalAlign='middle'>\n                        <Grid.Column style={{ maxWidth: 450 }}>                     \n                        <Form inverted onSubmit={this.handleSubmitHabit}>       \n                            <Segment inverted>\n                                    <h2 style={{ color: 'white' }}>What habit would you like to 'LESSEN'?</h2>\n                                    <Form.Input\n                                        placeholder='Example: Eating potato chips for breakfast'\n                        