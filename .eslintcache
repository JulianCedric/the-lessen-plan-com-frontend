ningCount":0,"usedDeprecatedRules":"44"},"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/index.js",[],["88","89"],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/reportWebVitals.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/App.js",["90","91"],"import React from 'react';\nimport './App.css';\nimport { BrowserRouter as Router, Route, Switch, Redirect } from 'react-router-dom';\nimport { Sticky } from 'semantic-ui-react';\nimport Navbar from './Navbar';\nimport Dashboard from './Dashboard';\nimport Login from './Login';\nimport Lab from './Lab';\nimport Lab2 from './Lab2';\nimport Home from './Home';\nimport Footer from './Footer';\n\nclass App extends React.Component {\n  state = {  \n    loggedIn: false\n  }\n\n  setLogin = e => {\n    console.log('setLogin')\n    this.setState({ loggedIn: !this.state.loggedIn })\n  } \n\n  render() { \n    return (  \n      <Router>\n        <React.Fragment>\n          <Switch>\n            <div className=\"App\" style={{ color: 'white' }}>\n              <Sticky context={this.contextRef}>\n                <Navbar loggedIn={this.state.loggedIn} setLogin={this.setLogin}/>\n              </Sticky>\n              <Route path='/' render={(props) => (<Home {...props} />)}></Route>\n              <Route path='/lab' render={(props) => (<Lab {...props} />)}></Route>\n              {/* <Route path='/lab2' render={(props) => (<Lab2 {...props} />)}></Route> */}\n              <Route path='/login' render={(props) => (<Login {...props} setLogin={this.setLogin}/>)}></Route>\n            \n              {!this.state.loggedIn \n                ? <Redirect to=\"/\" component={ App } /> \n                : <Redirect to=\"/lab\" component={ Lab } /> \n              }\n            \n            </div>\n          </Switch>\n          <Footer />\n        </React.Fragment>\n      </Router>\n    );\n  }\n}\n \nexport default App;","/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Dashboard.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/CreateForm.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/BadLoop.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/GoodLoop.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Navbar.js",["92","93","94"],"import React from 'react';\nimport { Link } from 'react-router-dom';\nimport { Icon } from 'semantic-ui-react';\n\nclass Navbar extends React.Component {\n    render() { \n        return (  \n            <div className=\"Navbar\">\n                <div className=\"ui inverted borderless large menu\">\n                    <a className=\"item\">\n                        <Link to=\"/\" className=\"link\"><Icon name='home' size='big'></Icon></Link>\n                    </a>\n                    \n                    <a class='item'>\n                        <Link to='/lab' className='lab'><Icon name='lab' size='big'></Icon></Link>\n                    </a>\n\n                    {/* <a class='item'>\n                        <Link to='/lab2' className='lab2'><Icon name='book' size='big'></Icon></Link>\n                    </a> */}\n\n                    <div className=\"right menu\">\n                        <a className=\"item\">\n                            {\n                                this.props.loggedIn\n                                ? <Link to=\"/login\" onClick={this.props.setLogin} className=\"link\">Logout</Link>\n                                : <Link to=\"/login\" className=\"link\">Login | Sign Up</Link>\n                            }\n                        </a>\n                    </div>\n                </div>\n            </div>\n        );\n    };\n};\n\nexport default Navbar;","/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Login.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/HabitLoop.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/FunctionMerge.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/LessenPlan.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Lab.js",["95","96","97","98","99","100","101","102","103","104"],"import React from 'react';\nimport { Button, Form, Grid, Icon, Image, Reveal } from 'semantic-ui-react';\nimport HabitLoop from './HabitLoop';\nimport FunctionMerge from './FunctionMerge';\nimport LessenPlan from './LessenPlan';\nimport ReactToPrint from \"react-to-print\";\nimport Create from './Create';\nimport LessenPlans from './LessenPlans';\nimport Update from './Update';\nimport LabIcon from './LabIcon';\n\nconst USERS = [\n    {\n        user_id: 1,\n        name: 'Julian'\n    },\n    {\n        user_id: 2,\n        name: 'Cedric'\n    }\n]\n\nconst LESSENPLANS = [\n    {\n      id: 0,\n      user_id: 0,\n      type: 'morning',\n      habitDesc: '',\n      habitBad: false,\n      habitLoopItemA: 'Cue',\n      habitLoopItemB: 'Craving',\n      habitLoopItemC: 'Response',\n      habitLoopItemD: 'Reward',\n      habitTracker: 'Under Construction'\n    }\n  ]\n\nclass Lab extends React.Component {\n    state = {  \n        users: [],\n        lessenPlans: [],\n        renderCreate: true,\n        renderUpdate: false\n    }\n\n    // badHabit = e => {\n    //     console.log('Confirmation: badHabit()')\n    //     this.setState({ habitBad: true })\n    // }\n\n    create = obj => {\n        let arr = [...this.state.lessenPlans, obj]\n        this.setState({ lessenPlans: arr })\n    }\n\n    renderCreate = e => {\n        this.setState({ renderCreate: !this.state.renderCreate })\n        this.setState({ renderUpdate: !this.state.renderUpdate })\n    }\n\n    updateHabitDesc = (id, desc, bad) => {\n        let arr = [...this.state.lessenPlans]\n        let obj = arr.find(elem => elem.id === id)\n\n        console.log('id: ', id)\n        console.log('desc: ', desc)\n\n        obj.habitDesc = desc\n        obj.habitBad = bad\n\n        this.setState({ lessenPlans: arr })\n    }\n\n    updateHabitLoopItemA = (id, cue) => {\n        let arr = [...this.state.lessenPlans]\n        let obj = arr.find(elem => elem.id === id)\n        obj.habitLoopItemA = cue\n        this.setState({ lessenPlans: arr })\n    }\n\n    handleClickLoadUsers = e => {\n        console.log('[ Load Users ]')\n        this.setState({ users: USERS })\n    }\n\n    handleClickLoadLessenPlans = e => {\n        console.log('[ Load Lessen Plans ]')\n        this.setState({ lessenPlans: LESSENPLANS })\n    }\n\n    handleClickMerge = (a,b) => {\n        console.log('[ Merge ]')\n    }\n\n    render() { \n        console.log(\"Current state of 'lessenPlans': \", this.state.lessenPlans)\n        return (  \n            <div className=\"lab\" style={{ color: 'white' }}>\n                <LabIcon />\n                <h2>Create a New Lessen Plan</h2> \n\n                {this.state.renderCreate ? <Create create={this.create} renderCreate={this.renderCreate} /> : null }\n\n                {this.state.renderUpdate ? <Update updateHabitDesc={this.updateHabitDesc} updateHabitLoopItemA={this.updateHabitLoopItemA} badHabit={this.badHabit} /> : null }\n\n                <br/>\n\n                <br/>\n\n                {/* <FunctionMerge users={this.state.users} lessenPlans={this.state.lessenPlans}/> */}\n                <br/>\n                <br/>\n\n                {/* <Button onClick={this.handleClickMerge}>Merge</Button> */}\n\n                <LessenPlans lessenPlans={this.state.lessenPlans} />\n\n                <br/>\n                <br/>\n                <br/>\n                <br/>\n                <br/>\n\n                {/* <LessenPlan ref={(el) => (this.componentRef = el)} />\n                \n                <span>\n                    <Icon color='blue' name='save' size='big'></Icon>\n                    <ReactToPrint \n                        trigger={() => <a href=\"#\"><Icon color='blue' name='print' size='big'></Icon></a>}\n                        content={() => this.componentRef}\n                    />\n                </span> */}\n\n                \n            </div>\n        );\n    }\n}\n \nexport default Lab;","/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/LessenPlans.js",["105","106"],"import React from 'react';\nimport LessenPlan from './LessenPlan';\nimport { Icon, Item } from 'semantic-ui-react';\nimport ReactToPrint from \"react-to-print\";\n\nclass LessenPlans extends React.Component {\n    state = {  }\n    render() { \n        return (  \n            <div>\n                {this.props.lessenPlans.map(lessenPlan => <LessenPlan key={lessenPlan.id} lessenPlan={lessenPlan} ref={(el) => (this.componentRef = el)} />)}\n            \n            \n                <span>\n                    <Icon color='blue' name='save' size='big'></Icon>\n                    <ReactToPrint \n                        trigger={() => <a href=\"#\"><Icon color='blue' name='print' size='big'></Icon></a>}\n                        content={() => this.componentRef}\n                    />\n                </span>\n\n            </div>\n        );\n    }\n}\n \nexport default LessenPlans;","/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Update.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Create.js",["107"],"import React from 'react';\nimport { Button, Form } from 'semantic-ui-react';\n\nclass Create extends React.Component {\n    state = {  \n        id: 1,\n        user_id: 1,\n        type: \"\",\n        habitDesc: \"\",\n        habitBad: false,\n        habitLoopItemA: \"\",\n        habitLoopItemB: \"\",\n        habitLoopItemC: \"\",\n        habitLoopItemD: \"\",\n        habitTracker: \"\"\n    }\n\n    handleChange = e => {\n        this.setState({ [e.target.name]: e.target.value })\n    }\n\n    handleSubmit = e => {\n        let obj = this.state\n        this.props.create(obj)\n        this.props.renderCreate()\n    }\n\n    render() { \n        return (  \n\n            <div>\n                <hr/>\n                <br/>\n                <div className=\"create\">\n                    \n                    <h3>What would you like it's title to be?</h3>\n                    <Form onSubmit={this.handleSubmit}>\n                        <Form.Group>\n                            <Form.Input \n                                placeholder=\"e.g. Morning Routine\"\n                                name=\"type\"\n                                value={this.state.type}\n                                onChange={this.handleChange}\n                                style={{ width: '500px' }}\n                            />\n                        </Form.Group>\n                        <Form.Button color='blue' content='Create' style={{ width: '500px' }}/>\n                    </Form>\n                </div>\n            </div>\n        );\n    }\n}\n \nexport default Create;","/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/LabIcon.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Lab2.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Home.js",[],"/Users/JulianREIMAGINED/Desktop/Coding/the-lessen-plan-com-frontend/src/Footer.js",[],{"ruleId":"108","replacedBy":"109"},{"ruleId":"110","replacedBy":"111"},{"ruleId":"112","severity":1,"message":"113","line":6,"column":8,"nodeType":"114","messageId":"115","endLine":6,"endColumn":17},{"ruleId":"112","severity":1,"message":"116","line":9,"column":8,"nodeType":"114","messageId":"115","endLine":9,"endColumn":12},{"ruleId":"117","severity":1,"message":"118","line":10,"column":21,"nodeType":"119","endLine":10,"endColumn":41},{"ruleId":"117","severity":1,"message":"118","line":14,"column":21,"nodeType":"119","endLine":14,"endColumn":37},{"ruleId":"117",